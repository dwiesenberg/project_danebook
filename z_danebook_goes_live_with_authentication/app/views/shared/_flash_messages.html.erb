<% flash.each do |type, msg| %>
  <% if msg.class == Array %>
    <% msg.each do |string| %>
      <%= render partial: 'shared/flash_message', locals: { msg: string, type: type } %>
    <% end %>
  <% else %>
    <%= render partial: 'shared/flash_message', locals: { msg: msg, type: type } %>
  <% end %>
<% end %>
